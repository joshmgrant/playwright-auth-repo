{
  "name": "app-frontend",
  "type": "module",
  "version": "2.4.3",
  "private": true,
  "scripts": {
    "serve": "vite preview --port 4000",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src",
    "lint:ci": "eslint src",
    "dev": "vite --port 4000",
    "dev-docker": "vite --host 0.0.0.0 --port 4400",
    "e2e-install": "pnpm exec playwright install-deps && pnpm exec playwright install",
    "e2e-install:ci": "pnpm exec playwright install-deps && pnpm exec playwright install chrome",
    "test:unit": "vitest",
    "test:e2e": "playwright test"
  },
